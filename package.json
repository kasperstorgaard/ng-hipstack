{
  "name": "ng-hipstack",
  "version": "0.1.0",
  "description": "angular 2 hip stack",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "postinstall": "typings install",
    "server": "http-server",
    "start": "npm run clean && concurrently \"npm run webpack:w\" \"npm run server\"",
    "typings": "typings",
    "webpack": "webpack",
    "webpack:w": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kasperstorgaard/ng-hipstack.git"
  },
  "keywords": [
    "angular2",
    "hip",
    "stack",
    "tape",
    "testdouble.js"
  ],
  "author": "kasperstorgaard",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kasperstorgaard/ng-hipstack/issues"
  },
  "homepage": "https://github.com/kasperstorgaard/ng-hipstack#readme",
  "devDependencies": {
    "concurrently": "^2.2.0",
    "http-server": "^0.9.0",
    "lite-server": "^2.2.2",
    "source-map-loader": "^0.1.5",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "typescript": "~1.8.10",
    "typings": "^1.3.3",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "@angular/common": "^2.0.0-rc.5",
    "@angular/compiler": "^2.0.0-rc.5",
    "@angular/core": "^2.0.0-rc.6",
    "@angular/forms": "^0.3.0",
    "@angular/http": "^2.0.0-rc.6",
    "@angular/platform-browser": "^2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.6",
    "@angular/router": "^3.0.0-rc.1",
    "angular2-in-memory-web-api": "0.0.18",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.11",
    "typescript": "^2.0.2",
    "zone.js": "^0.6.17"
  }
}
